{
   "expo": {
      "name": "cloo",
      "slug": "cloo",
      "version": "1.0.0",
      "orientation": "portrait",
      "icon": "./assets/images/icon.png",
      "scheme": "myapp",
      "userInterfaceStyle": "automatic",
      "newArchEnabled": true,
      "experiments": {
         "reactCompiler": true
      },
      "splash": {
         "image": "./assets/images/cloo_logo.png",
         "resizeMode": "contain",
         "backgroundColor": "#0f1014"
      },
      "assetBundlePatterns": ["**/*"],
      "ios": {
         "supportsTablet": true
      },
      "android": {
         "adaptiveIcon": {
            "foregroundImage": "./assets/images/cloo_logo.png",
            "backgroundColor": "#0f1014"
         }
      },
      "web": {
         "bundler": "metro",
         "output": "static",
         "favicon": "./assets/images/favicon.png"
      },
      "plugins": ["expo-router"],
      "experiments": {
         "typedRoutes": true
      }
   }
}
